#!/bin/bash

set -x
ln -svf Makefile-without-docker Makefile

exit 0


if [ "${SHIPPABLE}" = "true" -o "${SEMAPHORE}" = "true" ]
then
	ln -svf Makefile-docker Makefile
	docker ps -a
	docker images
	git clone https://github.com/jdanecki/kanapi.git bbs/kanapi
	cd bbs/kanapi; git show ; cd ../..
else
	ln -svf Makefile-without-docker Makefile
fi

ls -l Makefile

#set


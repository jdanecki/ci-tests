#!/bin/bash

set -x

if [ "${SHIPPABLE}" = "true" -o "${SEMAPHORE}" = "true" -o "${TRAVIS}" = "true" ]
then
	ln -svf Makefile-docker Makefile
	docker ps -a
	docker images
	git clone https://github.com/jdanecki/kanapi.git bbs/kanapi
	cd bbs/kanapi; git show
else
	ln -svf Makefile-codeship Makefile
fi

ls -l Makefile

#set


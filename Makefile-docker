all: x11_build

sbs_build:
	cd sbs; ./build ; if [ "$$?" = "0" ] ; then cd .. ; else false; fi

bbs_build:
	cd bbs; ./build ; if [ "$$?" = "0" ] ; then cd .. ; else false; fi

llvm_build:
	cd llvm; ./build ; if [ "$$?" = "0" ] ; then cd .. ; else false; fi

x11_build:
	cd x11; ./build ; if [ "$$?" = "0" ] ; then cd .. ; else false; fi

test:
	docker ps -a
	docker images

